    "Rijk van Zanten <rijkvanzanten@me.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/directus/example-stock-interface/issues"
  },
  "homepage": "https://github.com/directus/example-stock-interface#readme",
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.3.1",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.15.2",
    "vue-template-compiler": "^2.5.21"
  },
  "dependencies": {
    "@directus/mixins": "^1.1.0",
    "chart.js": "^2.7.3",
    "hex-to-rgba": "^1.0.2",
    "number-abbreviate": "^2.0.0"
  }
}
