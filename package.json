{
  "name": "example-stock-interface",
  "version": "1.0.0",
  "description": "An example custom interface for Directus 7",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:input && npm run build:display && npm run build:meta",
    "build:input": "parcel build src/input.vue",
    "build:display": "parcel build src/display.vue",
    "build:meta": "cp src/meta.json dist/meta.json"
  },
  "repository": "directus/example-stock-interface",
  "keywords": [
    "example",
    "interface",
    "directus",
    "stock"
  ],
